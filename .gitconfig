[user]
	name = yubo
	email = yubo@yubo.org
[diff]
	tool = vimdiff
[difftool]
	prompt = No
[core]
	editor = vim
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
[push]
	default = matching
[color]
	ui = auto
